<div class="container">
  <nav class="breadcrumb" aria-label="breadcrumbs">
    <ul>
      <li><%= link_to "Home", '/' %></li>
      <li><%= link_to "Chord Modifiers", chord_modifiers_path %></li>
      <li class="is-active"><%= link_to @chord.chord_modifier_name, chord_modifier_chord_modifiers_chords_path(@chord.chord_modifier_name) %></li>
    </ul>
  </nav>
</div>
<div class="container">
  <% @chords.each do |chord| %>
    <p>
      <%= chord.chord_name_with_flat %>
      <%= link_to chord.chord_name_with_flat, chord_modifier_chord_modifiers_chord_path(chord.chord_modifier_name, chord.chord_name_with_flat) %>
    </p>
  <% end %>
</div>
